{
  "name": "birch-markdown",
  "main": "./lib/birch-markdown",
  "version": "0.0.0",
  "description": "Author Markdown in the Birch outliner",
  "activationCommands": {
    "birch-outline-editor": [
      "birch-markdown:copy-as-markdown"
    ],
    "birch-outline-editor.outlineMode": [
      "birch-markdown:make-paragraph",
      "birch-markdown:make-heading",
      "birch-markdown:make-ordered-list",
      "birch-markdown:make-unordered-list",
      "birch-markdown:make-code-block",
      "birch-markdown:make-block-quote"
    ]
  },
  "consumedServices": {
    "birch-outine-editor-service": {
      "versions": {
        "^0.1.0": "consumeBirchOutlineEditorService"
      }
    }
  },
  "repository": "https://github.com/FoldingText/birch-markdown",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "to-markdown": "0.0.3"
  }
}